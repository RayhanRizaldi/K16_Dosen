<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tittleins"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".inputnilaisidangActivity">

    <ImageView
        android:id="@+id/profilins"
        android:layout_width="64dp"
        android:layout_height="54dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="36dp"
        android:contentDescription="TODO"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/avatar" />

    <TextView
        android:id="@+id/namains"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/Mnama"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/profilins"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nimins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/nim"
        android:textColor="#000000"
        android:textColorHint="#FFFFFF"
        android:textIsSelectable="false"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/profilins"
        app:layout_constraintTop_toBottomOf="@+id/namains" />

    <Button
        android:id="@+id/buttonINS"
        android:layout_width="149dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button"
        android:backgroundTint="@color/oren"
        android:drawableLeft="@drawable/input"
        android:text="@string/btn_innilai"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="@+id/nimins"
        app:layout_constraintTop_toBottomOf="@+id/nimins"
        tools:ignore="TouchTargetSizeCheck"
        android:onClick="buttonINS"/>

    //Judul Tugas Akhir

    <TextView
        android:id="@+id/JudulTAins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/JudulTA"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonINS" />

    <EditText
        android:id="@+id/isiJTAins"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="none"
        android:enabled="false"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintStart_toStartOf="@+id/JudulTAins"
        app:layout_constraintTop_toBottomOf="@+id/JudulTAins"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
        android:autofillHints="" />

    //Dosen pembimbing

    <TextView
        android:id="@+id/Dospemins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/Dospem"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/JudulTAins"
        app:layout_constraintTop_toBottomOf="@+id/isiJTAins" />

    <EditText
        android:id="@+id/isiDPins"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="none"
        android:enabled="false"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintStart_toStartOf="@+id/Dospemins"
        app:layout_constraintTop_toBottomOf="@+id/Dospemins"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"/>

    // Jadwal

    <TextView
        android:id="@+id/jadwalins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/sjadwal"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/Dospemins"
        app:layout_constraintTop_toBottomOf="@+id/isiDPins" />

    <EditText
        android:id="@+id/isijadwalins"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="start|top"
        android:inputType="none"
        android:minHeight="?attr/actionBarSize"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/jadwalins"
        app:layout_constraintTop_toBottomOf="@+id/jadwalins"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    //Lokasi

    <TextView
        android:id="@+id/lokasiins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/lokasi"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/jadwalins"
        app:layout_constraintTop_toBottomOf="@+id/isijadwalins" />

    <EditText
        android:id="@+id/isiLokasiins"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="start|top"
        android:inputType="none"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/lokasiins"
        app:layout_constraintTop_toBottomOf="@+id/lokasiins"
        tools:ignore="RtlHardcoded,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/dospengins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/dospeng"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/lokasiins"
        app:layout_constraintTop_toBottomOf="@+id/isiLokasiins" />

    <EditText
        android:id="@+id/isidospengins"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="15dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="start|top"
        android:inputType="none"
        app:layout_constraintStart_toStartOf="@+id/dospengins"
        app:layout_constraintTop_toBottomOf="@+id/dospengins"
        tools:ignore="RtlHardcoded,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    //Detail TA

    <TextView
        android:id="@+id/DetailTAins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/file_dta"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/dospengins"
        app:layout_constraintTop_toBottomOf="@+id/isidospengins" />

    <ImageView
        android:id="@+id/fileins"
        android:layout_width="79dp"
        android:layout_height="96dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/DetailTAins"
        app:layout_constraintTop_toBottomOf="@+id/DetailTAins"
        app:srcCompat="@drawable/pdf" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fileins"
        app:layout_constraintEnd_toEndOf="@+id/fileins"
        app:layout_constraintStart_toStartOf="@+id/fileins"
        app:layout_constraintTop_toTopOf="@+id/fileins"
        app:srcCompat="@drawable/unduh" />





</androidx.constraintlayout.widget.ConstraintLayout>